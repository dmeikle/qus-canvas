      "import": "./dist/courses/config/endpoints.js",
      "require": "./dist/courses/config/endpoints.js",
      "types": "./dist/courses/config/endpoints.d.ts"
    },
    "./courses/dtos/course.dto": {
      "import": "./dist/courses/dtos/course.dto.js",
      "require": "./dist/courses/dtos/course.dto.js",
      "types": "./dist/courses/dtos/course.dto.d.ts"
    },
    "./courses/factories/courses.factory": {
      "import": "./dist/courses/factories/courses.factory.js",
